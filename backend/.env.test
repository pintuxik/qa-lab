# Backend Unit Test Environment Configuration
# Loaded automatically by pytest-dotenv before tests run.
# Unit tests use in-memory SQLite, so DB_* values are placeholders.

# Testing flag
TESTING=true

# Test mode disabled for unit tests (not needed with in-memory DB)
TEST_MODE_ENABLED=false

# Database config (placeholders - unit tests use in-memory SQLite)
DB_HOST=unit
DB_PORT=0
DB_NAME=unit
DB_USER=unit
DB_PASSWORD=unit

# Application config
SECRET_KEY=unit
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=1
FRONTEND_URL=unit
